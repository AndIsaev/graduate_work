root:
    $Find $Current? $Film

filler:
    %lemma
    алиса
    пожалуйста
    я смотрю

$Find:
    %lemma
    скажи
    назови
    поищи
    найди
    найти
    подскажи
    расскажи

$Current:
    %lemma
    (инфу|информацию)? (о|об) (этом|данном)
    что (это)? за

$Film:
    %lemma
    (фильм|кино|картина|лента|кинолента|муви)+
