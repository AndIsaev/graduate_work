root:
    $Find $Current? $Film

filler:
    %lemma
    алиса
    пожалуйста

slots:
    film:
        source: $Film

$Find:
    %lemma
    скажи
    назови
    поищи
    найди
    найти
    подскажи
    расскажи

$Current:
    %lemma
    (инфу|информацию)? (о|об) (фильм|кино|картина|лента|кинолента|муви)+
    что за (фильм|кино|картина|лента|кинолента|муви)+

$Film:
    .+
