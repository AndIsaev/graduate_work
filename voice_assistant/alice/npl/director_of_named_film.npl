root:
    $Find? $What? $Director $Made? $Of $Film

filler:
    %lemma
    алиса
    пожалуйста

slots:
    film:
        source: $Film

$Film:
    .+

$Of:
    %lemma
    (этот|данный)? (фильм|кино|лента|произведение|кинопроизведение|муви)+

$What:
    %lemma
    какой

$Director:
    %lemma
    [главный? pежиссёр|директор]
    кто

$Made:
    %lemma
    срежиссировал
    снял

$Find:
    %lemma
    скажи
    назови
    поищи
    найди
    найти
    подскажи
